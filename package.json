    "eslint-plugin-typescript-sort-keys": "^1.8.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "jest": "^27.0.6",
    "pkg-dir": "^5.0.0",
    "prettier": "^2.3.2",
    "tempy": "^1.0.1",
    "typescript": "^4.3.5",
